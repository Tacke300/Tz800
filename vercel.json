{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram_webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram_webhook",
      "dest": "/api/telegram_webhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
